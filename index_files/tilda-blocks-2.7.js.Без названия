function t902_init(recid){var el=$('#rec'+recid);t902_unifyHeights(recid);$(window).bind('resize',t_throttle(function(){t902_unifyHeights(recid)}));el.find('.t902').bind('displayChanged',function(){t902_unifyHeights(recid)})}
